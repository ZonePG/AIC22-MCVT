        {
            "name": "Python: gen_image_aic",
            "type": "python",
            "request": "launch",
            "program": "gen_images_aic.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/detector",
            "args": [
                "aic_all.yml"
            ]
        },
        {
            "name": "Python: detect2img c041",
            "type": "python",
            "request": "launch",
            "program": "detect2img.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/detector/yolov5_2022",
            "args": [
                "--name=c041",
                "--weights=./weights/yolov5x6.pt",
                "--conf-thres=0.1",
                "--agnostic",
                "--save-txt",
                "--save-conf",
                "--img-size=1280",
                "--classes", "2", "5", "7",
                "--cfg_file=aic_all.yml",
            ],
